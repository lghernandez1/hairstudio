<form class="form-horizontal" >
<fieldset>

<!-- Form Name -->
<h1>Make an Appointment</h1>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="textinput-0">First name</label>
  <div class="controls">
    <input id="textinput-0" name="textinput-0" type="text" placeholder="First Name" class="input-xlarge">
    
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="textinput-1">Last Name</label>
  <div class="controls">
    <input id="textinput-1" name="textinput-1" type="text" placeholder="Last Name" class="input-xlarge">
    
  </div>
</div>

<!-- Multiple Radios -->
<div class="control-group">
  <label class="control-label" for="multipleradios-0"><ul>Gender</ul></label>
  <div class="controls">
    <label class="radio" for="multipleradios-0-0">
      <input type="radio" name="multipleradios-0" id="multipleradios-0-0" value="Female" checked="checked">
      Female
    </label>
    <label class="radio" for="multipleradios-0-1">
      <input type="radio" name="multipleradios-0" id="multipleradios-0-1" value="Male">
      Male
    </label>
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="textinput-2">Date</label>
  <div class="controls">
    <input id="textinput-2" name="textinput-2" type="text" placeholder="Date " class="input-xlarge">
    
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="textinput-3">Time</label>
  <div class="controls">
    <input id="textinput-3" name="textinput-3" type="text" placeholder="Time" class="input-xlarge">
    
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="textinput-5">Phone Number</label>
  <div class="controls">
    <input id="textinput-5" name="textinput-5" type="text" placeholder="Phone Number" class="input-xlarge">
    
  </div>
</div>

<!-- Text input-->
<div class="control-group">
  <label class="control-label" for="textinput-4">Email Address</label>
  <div class="controls">
    <input id="textinput-4" name="textinput-4" type="text" placeholder="Email" class="input-xlarge">
    
  </div>
</div>

<!-- Textarea -->
<div class="control-group">
  <label class="control-label" for="textarea-1">Service</label>
  <div class="controls">                     
    <textarea id="textarea-1" name="textarea-1"></textarea>
  </div>
</div>

<!-- Button -->
<div class="control-group">
  <label class="control-label" for="singlebutton-0"></label>
  <div class="controls">
    <button id="singlebutton-0" name="singlebutton-0" class="btn btn-primary">Submit</button>
  </div>
</div>

</fieldset>
</form>
